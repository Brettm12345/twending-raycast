{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "twending",
  "title": "Twending",
  "description": "A new way to view github trending",
  "icon": "icon.png",
  "author": "brettm12345",
  "ai": {
    "evals": [
      {
        "input": "@twending Show trending repositories for the default language for the last week",
        "expectedOutput": "The list of trending repositories for the default language for the last week",
        "usedAsExample": true
      },
      {
        "input": "@twending Show me a list of the trending repositories and give me a detailed summary of each trend for the date range period",
        "expectedOutput": "The list of trending repositories and a detailed summary of each trend for the date range period",
        "usedAsExample": true
      },
      {
        "input": "@twending Show trending repositories for TypeScript for the last week",
        "expectedOutput": "The list of trending repositories for TypeScript for the last week",
        "usedAsExample": true
      }
    ]
  },
  "preferences": [
    {
      "name": "githubToken",
      "type": "password",
      "required": false,
      "title": "GitHub Personal Access Token",
      "description": "The personal access token to use to access the GitHub API",
      "placeholder": "ghp_1234567890"
    },
    {
      "name": "defaultPeriod",
      "label": "Default Period",
      "title": "Default Period",
      "required": false,
      "description": "The default period to use if skip period selection is enabled",
      "type": "dropdown",
      "default": "weekly",
